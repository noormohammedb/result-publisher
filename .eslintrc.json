{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-console": "off",
    "indent": ["error", 3],
    "no-multi-assign": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off"
  }
}
